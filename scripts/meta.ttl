<http://hdl.handle.net/11336/33299> dblp:publishedInJournal <http://data.cd.gob.ar/resource/journal/electronic-commerce-research> ;
	dblp:isbn "1389-5753"^^xsd:string ;
	dblp:publicationType "info:eu-repo/semantics/article"^^xsd:string ;
